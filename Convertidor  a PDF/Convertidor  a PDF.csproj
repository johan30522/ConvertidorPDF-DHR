    <TargetFramework>net8.0</TargetFramework>
    <RootNamespace>ConvertidorPDF</RootNamespace>
    <AssemblyName>ConvertidorPDF</AssemblyName>
    <ImplicitUsings>enable</ImplicitUsings>
